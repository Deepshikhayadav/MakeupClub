<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MakeupClub" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_vari</item>
        <item name="colorPrimaryVariant">@color/surface_dark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/deep_purple_200</item>
        <item name="colorSecondaryVariant">@color/deep_purple_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!--onSurface color-->
        <item name="colorSurface">@color/surface_dark</item>
        <item name="colorOnSurface">@color/primary_text</item>
        <!--Background color-->
        <item name="android:colorBackground">@color/background</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>